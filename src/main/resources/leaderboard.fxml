<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boardgame.scene.LeaderboardController">
   <children>
      <VBox prefHeight="840.0" prefWidth="840.0">
         <children>
            <TextFlow fx:id="leaderboardTextFlow" minHeight="740.0" minWidth="840.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </TextFlow>
            <HBox alignment="CENTER" minHeight="100.0" minWidth="840.0" spacing="50.0">
               <children>
                  <Button minHeight="40.0" minWidth="80.0" mnemonicParsing="false" onAction="#delete" text="Delete">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding></Button>
                  <Button minHeight="40.0" minWidth="80.0" mnemonicParsing="false" text="Menu">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding></Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
