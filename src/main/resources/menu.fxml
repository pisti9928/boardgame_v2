               </padding>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Button fx:id="exit" mnemonicParsing="false" onAction="#leaveGame" text="Exit Game" textAlignment="CENTER">
